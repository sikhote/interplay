{
		"name": "clairic",
		"description": "Consume media from the cloud on a browser",
		"author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
		"homepage": "clairic.com",
		"version": "0.0.1",
		"private": true,
		"scripts": {
				"deploy": "serverless deploy",
				"destroy": "serverless remove",
				"eslint": "eslint \"./**/*.js\"",
				"export": "next build && next export -o build",
				"invalidate": "serverless invalidate",
				"next": "next",
				"prettier": "prettier --write \"./**/*.js\"",
				"server": "cd build && python -m SimpleHTTPServer",
				"sync": "serverless sync"
		},
		"dependencies": {
				"antd": "2.13.11",
				"babel-eslint": "8.0.1",
				"bluebird": "3.5.0",
				"dropbox": "2.5.9",
				"eslint": "4.11.0",
				"eslint-config-airbnb": "16.1.0",
				"eslint-config-prettier": "2.8.0",
				"eslint-plugin-import": "2.8.0",
				"eslint-plugin-jsx-a11y": "6.0.2",
				"eslint-plugin-react": "7.5.1",
				"lodash": "4.17.4",
				"material-ui": "0.19.3",
				"moment": "2.19.3",
				"next": "4.1.4",
				"node-sass": "^4.7.2",
				"polished": "1.7.0",
				"prettier": "1.8.2",
				"prop-types": "15.6.0",
				"ramda": "0.24.1",
				"react": "16.2.0",
				"react-dom": "16.2.0",
				"react-redux": "5.0.6",
				"react-virtualized": "9.12.0",
				"redux": "3.7.2",
				"redux-observable": "0.16.0",
				"redux-persist": "4.10.1",
				"redux-thunk": "2.2.0",
				"rxjs": "5.4.3",
				"serverless": "1.11.0",
				"styled-jsx": "2.2.1",
				"styled-jsx-plugin-sass": "^0.2.2",
				"thenby": "1.2.3"
		}
}
